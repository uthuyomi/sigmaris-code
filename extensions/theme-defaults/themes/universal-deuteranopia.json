{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Deuteranopia Safe",
	"semanticHighlighting": true,

	"colors": {
		"editor.background": "#F5F5F7",
		"editor.foreground": "#1F1F21",

		"editor.selectionBackground": "#D8DFF0",
		"editor.inactiveSelectionBackground": "#E3E7F4",
		"editor.selectionHighlightBackground": "#EDF0FA",

		"editorIndentGuide.background1": "#D1D1D4",
		"editorIndentGuide.activeBackground1": "#B1B1B6",

		"editorSuggestWidget.background": "#FFFFFF",
		"editorSuggestWidget.border": "#C7C7CC",
		"editorSuggestWidget.foreground": "#1F1F21",
		"editorSuggestWidget.selectedBackground": "#DDE4F3",

		"activityBar.background": "#E7E7EA",
		"activityBar.foreground": "#1F1F21",
		"activityBarBadge.background": "#3B6EBB",
		"activityBarBadge.foreground": "#FFFFFF",

		"sideBar.background": "#F5F5F7",
		"sideBar.foreground": "#2A2A2C",
		"sideBar.border": "#D6D6DA",

		"list.hoverBackground": "#E5EAF6",
		"list.activeSelectionBackground": "#D0D8EB",
		"list.inactiveSelectionBackground": "#E3E8F4",

		"tab.inactiveBackground": "#EFEFF2",
		"tab.activeBackground": "#FFFFFF",
		"tab.activeForeground": "#1F1F21",
		"tab.border": "#CECED3",

		"titleBar.activeBackground": "#EFEFF2",
		"titleBar.activeForeground": "#1F1F21",
		"titleBar.inactiveBackground": "#E8E8EC",
		"titleBar.inactiveForeground": "#7B7B81",

		"menu.background": "#FFFFFF",
		"menu.foreground": "#1F1F21",
		"menu.border": "#CECED3",

		"input.background": "#FFFFFF",
		"input.border": "#CECED3",
		"input.placeholderForeground": "#7B7B81",

		"statusBar.background": "#D4D4D8",
		"statusBar.foreground": "#1F1F21",
		"statusBar.border": "#C8C8CC",

		"button.background": "#A8863B",
		"button.foreground": "#FFFFFF",
		"button.hoverBackground": "#927535",

		"panel.border": "#D4D4D8",
		"panel.background": "#F4F4F6",

		"terminal.background": "#F5F5F7",
		"terminal.foreground": "#1F1F21",

		"terminalCursor.background": "#1F1F21",
		"terminalCursor.foreground": "#1F1F21",

		"terminal.ansiBlack": "#2A2A2C",
		"terminal.ansiRed": "#6D4A2E",
		"terminal.ansiGreen": "#A8863B",
		"terminal.ansiYellow": "#C0A95C",
		"terminal.ansiBlue": "#3B6EBB",
		"terminal.ansiMagenta": "#6C5A96",
		"terminal.ansiCyan": "#568FA8",
		"terminal.ansiWhite": "#ECECEE",

		"terminal.ansiBrightBlack": "#3A3A3C",
		"terminal.ansiBrightRed": "#815935",
		"terminal.ansiBrightGreen": "#B99745",
		"terminal.ansiBrightYellow": "#CFBB6C",
		"terminal.ansiBrightBlue": "#437CCF",
		"terminal.ansiBrightMagenta": "#7765A6",
		"terminal.ansiBrightCyan": "#61A4BA",
		"terminal.ansiBrightWhite": "#FFFFFF"
	},

	"tokenColors": [
		{
			"scope": "comment",
			"settings": { "foreground": "#7C7C80", "fontStyle": "italic" }
		},
		{
			"scope": "keyword",
			"settings": { "foreground": "#3B6EBB" }
		},
		{
			"scope": "variable",
			"settings": { "foreground": "#1F1F21" }
		},
		{
			"scope": ["string", "constant.character"],
			"settings": { "foreground": "#6D4A2E" }
		},
		{
			"scope": ["constant.numeric", "constant.language"],
			"settings": { "foreground": "#A8863B" }
		},
		{
			"scope": "storage.type",
			"settings": { "foreground": "#6C5A96" }
		},
		{
			"scope": "entity.name.function",
			"settings": { "foreground": "#3B6EBB" }
		},
		{
			"scope": ["entity.name.tag", "punctuation.definition.tag"],
			"settings": { "foreground": "#6C5A96" }
		}
	],

	"semanticTokenColors": {
		"variable": "#1F1F21",
		"parameter": "#454548",
		"type": "#3B6EBB",
		"class": "#3B6EBB",
		"property": "#A8863B",
		"enumMember": "#568FA8",
		"function": "#3B6EBB",
		"method": "#3B6EBB",
		"stringLiteral": "#6D4A2E",
		"numberLiteral": "#A8863B",
		"keyword": "#3B6EBB"
	}
}
