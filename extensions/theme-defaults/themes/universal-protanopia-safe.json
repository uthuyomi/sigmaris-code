{
	"$schema": "vscode://schemas/color-theme",
	"name": "Universal Protanopia Safe",
	"semanticHighlighting": true,

	"colors": {
		"editor.background": "#F4F4F4",
		"editor.foreground": "#1F1F1F",

		"editor.selectionBackground": "#C9D4E1",
		"editor.inactiveSelectionBackground": "#D8E0E9",
		"editor.selectionHighlightBackground": "#E4E9F0",

		"editorIndentGuide.background1": "#D1D1D1",
		"editorIndentGuide.activeBackground1": "#AFAFAF",

		"editorSuggestWidget.background": "#FFFFFF",
		"editorSuggestWidget.border": "#C8C8C8",
		"editorSuggestWidget.foreground": "#1F1F1F",
		"editorSuggestWidget.selectedBackground": "#D5E1ED",

		"activityBar.background": "#E6E6E6",
		"activityBar.foreground": "#1F1F1F",
		"activityBarBadge.background": "#2E5A9B",
		"activityBarBadge.foreground": "#FFFFFF",

		"sideBar.background": "#F4F4F4",
		"sideBar.foreground": "#2A2A2A",
		"sideBar.border": "#D5D5D5",

		"list.hoverBackground": "#E4EBF3",
		"list.activeSelectionBackground": "#CCD9E6",
		"list.inactiveSelectionBackground": "#E1E7EF",

		"tab.inactiveBackground": "#ECECEC",
		"tab.activeBackground": "#FFFFFF",
		"tab.activeForeground": "#1F1F1F",
		"tab.border": "#D0D0D0",

		"titleBar.activeBackground": "#ECECEC",
		"titleBar.activeForeground": "#1F1F1F",
		"titleBar.inactiveBackground": "#E6E6E6",
		"titleBar.inactiveForeground": "#7A7A7A",

		"menu.background": "#FFFFFF",
		"menu.foreground": "#1F1F1F",
		"menu.border": "#D0D0D0",

		"input.background": "#FFFFFF",
		"input.border": "#D0D0D0",
		"input.placeholderForeground": "#7A7A7A",

		"statusBar.background": "#D5D5D5",
		"statusBar.foreground": "#1F1F1F",
		"statusBar.border": "#C5C5C5",

		"button.background": "#6B5A2E",
		"button.foreground": "#FFFFFF",
		"button.hoverBackground": "#5D4E29",

		"panel.border": "#D1D1D1",
		"panel.background": "#F7F7F7",

		"terminal.background": "#F4F4F4",
		"terminal.foreground": "#1F1F1F",

		"terminalCursor.background": "#1F1F1F",
		"terminalCursor.foreground": "#1F1F1F",

		"terminal.ansiBlack": "#2A2A2A",
		"terminal.ansiRed": "#6B5A2E",
		"terminal.ansiGreen": "#3F7F4D",
		"terminal.ansiYellow": "#A89D62",
		"terminal.ansiBlue": "#2E5A9B",
		"terminal.ansiMagenta": "#5E5A8B",
		"terminal.ansiCyan": "#4F8A9B",
		"terminal.ansiWhite": "#EDEDED",

		"terminal.ansiBrightBlack": "#3A3A3A",
		"terminal.ansiBrightRed": "#7A6A3A",
		"terminal.ansiBrightGreen": "#4F975F",
		"terminal.ansiBrightYellow": "#BDB27A",
		"terminal.ansiBrightBlue": "#3569B5",
		"terminal.ansiBrightMagenta": "#6C679E",
		"terminal.ansiBrightCyan": "#5BA3B1",
		"terminal.ansiBrightWhite": "#FFFFFF"
	},

	"tokenColors": [
		{
			"scope": "comment",
			"settings": { "foreground": "#7A7A7A", "fontStyle": "italic" }
		},
		{
			"scope": "keyword",
			"settings": { "foreground": "#2E5A9B" }
		},
		{
			"scope": "variable",
			"settings": { "foreground": "#1F1F1F" }
		},
		{
			"scope": "string",
			"settings": { "foreground": "#6B5A2E" }
		},
		{
			"scope": ["constant.numeric", "constant.language"],
			"settings": { "foreground": "#3F7F4D" }
		},
		{
			"scope": "storage.type",
			"settings": { "foreground": "#5E5A8B" }
		},
		{
			"scope": "entity.name.function",
			"settings": { "foreground": "#2E5A9B" }
		},
		{
			"scope": ["entity.name.tag", "punctuation.definition.tag"],
			"settings": { "foreground": "#5E5A8B" }
		}
	],

	"semanticTokenColors": {
		"variable": "#1F1F1F",
		"parameter": "#444444",
		"type": "#2E5A9B",
		"class": "#2E5A9B",
		"property": "#3F7F4D",
		"enumMember": "#4F8A9B",
		"function": "#2E5A9B",
		"method": "#2E5A9B",
		"stringLiteral": "#6B5A2E",
		"numberLiteral": "#3F7F4D",
		"keyword": "#2E5A9B"
	}
}
