{
	"$schema": "vscode://schemas/color-theme",
	"name": "Sigmaris Sakura",
	"colors": {
		"editor.background": "#FFF7F9",
		"editor.foreground": "#33292B",

		"editor.inactiveSelectionBackground": "#F6DCE8",
		"editor.selectionBackground": "#F4C7DD",
		"editor.selectionHighlightBackground": "#F7E2ED",

		"editorIndentGuide.background1": "#E5C9D5",
		"editorIndentGuide.activeBackground1": "#CC9BB2",

		"editorSuggestWidget.background": "#FFF3F7",
		"editorSuggestWidget.border": "#E5C3D1",
		"editorSuggestWidget.foreground": "#403438",
		"editorSuggestWidget.selectedBackground": "#F4CCDD",

		"activityBar.background": "#F8DDE7",
		"activityBar.foreground": "#3A2E33",
		"activityBarBadge.background": "#E36E9A",
		"activityBarBadge.foreground": "#FFFFFF",

		"sideBar.background": "#FFF7FA",
		"sideBar.foreground": "#3A2E33",
		"sideBar.border": "#E3C5D4",
		"sideBarTitle.foreground": "#6F5A62",

		"list.hoverBackground": "#F3DFE9",
		"list.activeSelectionBackground": "#EEC3D8",
		"list.inactiveSelectionBackground": "#F3D7E4",

		"tab.inactiveBackground": "#FCEFF5",
		"tab.activeBackground": "#FFFFFF",
		"tab.activeForeground": "#2C2527",
		"tab.border": "#E1C3D0",

		"titleBar.activeBackground": "#FCEEF5",
		"titleBar.activeForeground": "#3A2E33",
		"titleBar.inactiveBackground": "#F8E8EF",
		"titleBar.inactiveForeground": "#7D6A70",

		"menu.background": "#FFF5F9",
		"menu.foreground": "#3A2E33",
		"menu.border": "#E7C9D7",

		"input.background": "#FFFFFF",
		"input.border": "#E3C5D4",
		"input.placeholderForeground": "#A38E96",

		"statusBar.background": "#EAC7D7",
		"statusBar.foreground": "#3A2E33",
		"statusBar.border": "#E1C3D0",
		"statusBarItem.hoverBackground": "#DEB2C8",

		"button.background": "#E67FA7",
		"button.foreground": "#FFFFFF",
		"button.hoverBackground": "#D56A92",

		"panel.border": "#E3C5D4",
		"panel.background": "#FFF6FA",

		"terminal.background": "#FFF7F9",
		"terminal.foreground": "#3A2E33",
		"terminalCursor.background": "#3A2E33",
		"terminalCursor.foreground": "#3A2E33",

		"terminal.ansiBlack": "#4B3940",
		"terminal.ansiBlue": "#7A60A8",
		"terminal.ansiCyan": "#7DB6C0",
		"terminal.ansiGreen": "#629B7C",
		"terminal.ansiMagenta": "#B46AA4",
		"terminal.ansiRed": "#CC4A78",
		"terminal.ansiWhite": "#FFEFF4",
		"terminal.ansiYellow": "#DDAF6C",
		"terminal.ansiBrightBlack": "#6B5560",
		"terminal.ansiBrightBlue": "#8B74C0",
		"terminal.ansiBrightCyan": "#96CAD1",
		"terminal.ansiBrightGreen": "#76B18F",
		"terminal.ansiBrightMagenta": "#C985B8",
		"terminal.ansiBrightRed": "#DE5F8F",
		"terminal.ansiBrightWhite": "#FFFFFF",
		"terminal.ansiBrightYellow": "#EAC77D"
	},

	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#B48CA3",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#CC4A78"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#3D2F34"
			}
		},
		{
			"scope": ["string", "constant.character"],
			"settings": {
				"foreground": "#E06796"
			}
		},
		{
			"scope": ["constant.numeric", "constant.language"],
			"settings": {
				"foreground": "#A24A6F"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#C55483"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#925479"
			}
		},
		{
			"scope": ["entity.name.tag", "punctuation.definition.tag"],
			"settings": {
				"foreground": "#C96C94"
			}
		}
	],

	"semanticHighlighting": true,

	"semanticTokenColors": {
		"variable": "#3B2D32",
		"parameter": "#5B3F4E",
		"type": "#7D5870",
		"class": "#7C5772",
		"property": "#A2648A",
		"enumMember": "#A46985",
		"function": "#8C5076",
		"method": "#8C5076",
		"stringLiteral": "#DD4E85",
		"numberLiteral": "#A24A6F",
		"keyword": "#CC4A78"
	}
}
