{
	"$schema": "vscode://schemas/color-theme",
	"name": "Sigmaris Sakura Mild",

	"colors": {
		"editor.background": "#F6F0F3",
		"editor.foreground": "#3A2F33",

		"editor.inactiveSelectionBackground": "#ECD7E2",
		"editor.selectionBackground": "#E6CEDC",
		"editor.selectionHighlightBackground": "#F1E3EB",

		"editorIndentGuide.background1": "#D8C1CC",
		"editorIndentGuide.activeBackground1": "#C8A9B7",

		"editorSuggestWidget.background": "#FAF3F6",
		"editorSuggestWidget.border": "#DCC4D0",
		"editorSuggestWidget.foreground": "#3A2F33",
		"editorSuggestWidget.selectedBackground": "#E3CAD8",

		"activityBar.background": "#EEDDE4",
		"activityBar.foreground": "#3A2F33",
		"activityBarBadge.background": "#D46994",
		"activityBarBadge.foreground": "#FFFFFF",

		"sideBar.background": "#F8F2F5",
		"sideBar.foreground": "#3A2F33",
		"sideBar.border": "#DAC5D0",
		"sideBarTitle.foreground": "#6A5A61",

		"list.hoverBackground": "#EEDFE7",
		"list.activeSelectionBackground": "#E2C6D4",
		"list.inactiveSelectionBackground": "#EED7E1",

		"tab.inactiveBackground": "#F7EEF2",
		"tab.activeBackground": "#FFFFFF",
		"tab.activeForeground": "#2F262A",
		"tab.border": "#D6C1CB",

		"titleBar.activeBackground": "#F7EEF3",
		"titleBar.activeForeground": "#3A2F33",
		"titleBar.inactiveBackground": "#F0E4EA",
		"titleBar.inactiveForeground": "#786870",

		"menu.background": "#FAF3F7",
		"menu.foreground": "#3A2F33",
		"menu.border": "#DCC6D3",

		"input.background": "#FFFFFF",
		"input.border": "#D8C4CF",
		"input.placeholderForeground": "#9A8A91",

		"statusBar.background": "#DEC4D2",
		"statusBar.foreground": "#3A2F33",
		"statusBar.border": "#D6C1CB",
		"statusBarItem.hoverBackground": "#D0ABBE",

		"button.background": "#D878A3",
		"button.foreground": "#FFFFFF",
		"button.hoverBackground": "#C36892",

		"panel.border": "#D9C4CE",
		"panel.background": "#F8F1F5",

		"terminal.background": "#F6F0F3",
		"terminal.foreground": "#3A2F33",
		"terminalCursor.background": "#3A2F33",
		"terminalCursor.foreground": "#3A2F33",

		"terminal.ansiBlack": "#4A3D44",
		"terminal.ansiBlue": "#7869A4",
		"terminal.ansiCyan": "#79AEB8",
		"terminal.ansiGreen": "#5D9078",
		"terminal.ansiMagenta": "#A8659A",
		"terminal.ansiRed": "#C04B74",
		"terminal.ansiWhite": "#F7EEF3",
		"terminal.ansiYellow": "#CF9F64",
		"terminal.ansiBrightBlack": "#6A5961",
		"terminal.ansiBrightBlue": "#8776B8",
		"terminal.ansiBrightCyan": "#8EC1C9",
		"terminal.ansiBrightGreen": "#6EAA8A",
		"terminal.ansiBrightMagenta": "#BC7FB2",
		"terminal.ansiBrightRed": "#D65B8A",
		"terminal.ansiBrightWhite": "#FFFFFF",
		"terminal.ansiBrightYellow": "#E3BE72"
	},

	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#AA8EA0",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#C04B74"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#3A2F33"
			}
		},
		{
			"scope": ["string", "constant.character"],
			"settings": {
				"foreground": "#D7648F"
			}
		},
		{
			"scope": ["constant.numeric", "constant.language"],
			"settings": {
				"foreground": "#9B4A70"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#B95887"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#8B5275"
			}
		},
		{
			"scope": ["entity.name.tag", "punctuation.definition.tag"],
			"settings": {
				"foreground": "#BD6B92"
			}
		}
	],

	"semanticHighlighting": true,

	"semanticTokenColors": {
		"variable": "#3A2F33",
		"parameter": "#5A3F4D",
		"type": "#735066",
		"class": "#725066",
		"property": "#9A6185",
		"enumMember": "#9F6683",
		"function": "#874E73",
		"method": "#874E73",
		"stringLiteral": "#D45082",
		"numberLiteral": "#9B4A70",
		"keyword": "#C04B74"
	}
}
